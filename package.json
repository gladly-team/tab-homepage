{
  "name": "tab-homepage",
  "description": "Homepage for Tab for a Cause (https://tab.gladly.io)",
  "version": "1.0.0",
  "author": "The Tab for a Cause Team",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "@reach/router": "^1.3.4",
    "bowser": "^2.11.0",
    "common-tags": "^1.8.0",
    "gatsby": "^2.24.83",
    "gatsby-plugin-facebook-pixel": "^1.0.5",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sitemap": "^2.4.17",
    "gatsby-plugin-typography": "^2.5.13",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-yaml": "^2.4.15",
    "lodash": "^4.17.20",
    "mdi-material-ui": "^6.20.0",
    "moment": "^2.29.1",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-slick": "^0.27.12",
    "react-transition-group": "^4.4.1",
    "react-twitter-embed": "^3.0.3",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "typography": "^0.16.19"
  },
  "keywords": [
    "tab",
    "cause",
    "extension",
    "charity"
  ],
  "license": "MPL-2.0",
  "scripts": {
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "format": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "test": "yarn run lint && jest --env=jsdom --coverage && yarn run test:codecov",
    "test:codecov": "if-env CI=true && codecov || echo 'Skipping test coverage logging.'",
    "test:watch": "jest --env=jsdom --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@material-ui/codemod": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-gatsby": "^0.5.14",
    "codecov": "^3.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "esm": "^3.2.25",
    "if-env": "^1.0.4",
    "jest": "^26.6.0",
    "jsdom": "^16.4.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|public|node_modules|.cache)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
