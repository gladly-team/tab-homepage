{
  "name": "tab-homepage",
  "description": "Homepage for Tab for a Cause (https://tab.gladly.io)",
  "version": "1.0.0",
  "author": "The Tab for a Cause Team",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@reach/router": "^1.1.1",
    "browser-detect": "^0.2.28",
    "common-tags": "^1.8.0",
    "gatsby": "^2.5.7",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-material-ui": "^1.2.5",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.37",
    "gatsby-transformer-yaml": "^2.1.12",
    "lodash": "^4.17.11",
    "mdi-material-ui": "^5.11.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-slick": "^0.24.0",
    "react-transition-group": "^4.0.1",
    "react-typography": "^0.16.19",
    "slick-carousel": "^1.8.1",
    "typography": "^0.16.19"
  },
  "keywords": [
    "tab",
    "cause",
    "extension",
    "charity"
  ],
  "license": "MPL-2.0",
  "scripts": {
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "format": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "test": "yarn run lint && jest --env=jsdom --coverage && yarn run test:codecov",
    "test:codecov": "if-env CI=true && codecov || echo 'Skipping test coverage logging.'",
    "test:watch": "jest --env=jsdom --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/codemod": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-gatsby": "^0.1.11",
    "codecov": "^3.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "if-env": "^1.0.4",
    "jest": "^24.8.0",
    "jsdom": "^15.1.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|public|node_modules|.cache)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
