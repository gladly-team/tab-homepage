{
  "name": "tab-homepage",
  "description": "Homepage for Tab for a Cause (https://tab.gladly.io)",
  "version": "1.0.0",
  "author": "The Tab for a Cause Team",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@fullpage/react-fullpage": "0.1.19",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.10.0",
    "@reach/router": "^1.3.4",
    "@storybook/cli": "^6.4.14",
    "@use-it/interval": "^1.0.0",
    "babel-plugin-react-require": "^3.1.3",
    "bowser": "^2.11.0",
    "clsx": "^1.1.1",
    "common-tags": "^1.8.2",
    "dotenv-extended": "^2.9.0",
    "gatsby": "^3.14.0",
    "gatsby-plugin-facebook-pixel": "^1.0.5",
    "gatsby-plugin-google-fonts-v2": "^1.0.0",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sentry": "^1.0.1",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-typography": "^3.14.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-json": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "gatsby-transformer-yaml": "^3.14.0",
    "lodash": "^4.17.21",
    "mdi-material-ui": "^6.0.0",
    "moment": "^2.29.1",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-alice-carousel": "^2.4.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-share": "^4.3.1",
    "react-slick": "^0.28.1",
    "react-transition-group": "^4.4.1",
    "react-typography": "^0.16.19",
    "rehype-react": "^6.0.0",
    "rehype-sanitize": "^4.0.0",
    "remark": "^13.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "slick-carousel": "^1.8.1",
    "typography": "^0.16.19"
  },
  "keywords": [
    "tab",
    "cause",
    "extension",
    "charity"
  ],
  "license": "MPL-2.0",
  "scripts": {
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "SHOW_CATS_PAGE=true GATSBY_TELEMETRY_DISABLED=1 GATSBY_GENERATE_CAUSE_PAGES=true gatsby develop",
    "format": "eslint --fix 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'",
    "serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "test": "yarn run lint && jest --env=jsdom --coverage --testPathIgnorePatterns=e2e-tests",
    "test:e2e": "dotenv-extended --defaults=./.env yarn jest --testPathPattern=e2e-tests.test --watchAll=false",
    "test:watch": "jest --env=jsdom --testPathIgnorePatterns=e2e-tests --watch",
    "deploy": "gatsby-plugin-s3 deploy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@luigiminardim/storybook-addon-globals-controls": "^0.1.3",
    "@material-ui/codemod": "^4.5.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/addons": "^6.4.14",
    "@storybook/api": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/components": "^6.4.14",
    "@storybook/core-events": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/react": "^6.4.14",
    "@storybook/theming": "^6.4.14",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-gatsby": "^1.14.0",
    "chromatic": "^6.4.1",
    "core-js": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.6",
    "esm": "^3.2.25",
    "gatsby-plugin-s3": "^0.3.8",
    "graphql": "^14.0.0",
    "if-env": "^1.0.4",
    "jest": "^27.4.7",
    "jquery": "^3.6.0",
    "jscodeshift": "^0.11.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.1.2",
    "prop-types": "^15",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-gatsby": "^0.0.3",
    "tab-e2e": "^2.0.2",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/e2eTests/**",
      "!**/__tests__/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|dist|docs|public|node_modules|.cache)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
